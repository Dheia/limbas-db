            $ellid = dftyp_tags($gtabid,$bzm,$ID,$form_name,$field_name,$pool_multimode,$typ);
            ################

            if($showframe) {
                echo "</DIV>";
            }

		# multiselect - only if not unique
        } elseif (!$gfield[$gtabid]["unique"][$bzm]) {
