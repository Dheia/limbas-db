                echo '</div>';
            }
            if($showframe) {
                echo "</DIV>";
            }
        }

		if($ellid){$svalue = implode(";",$ellid);}

		if($showframe) {
            echo "<INPUT TYPE=\"HIDDEN\" ID=\"" . $form_name . "\" NAME=\"" . $form_name . "\" VALUE=\"" . $svalue . "\"></div></td><td valign=\"top\">";
